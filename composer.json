{
    "name": "ovac/idoc",
    "license": "MIT",
    "description": "Generate beautiful API documentation from your Laravel application",
    "keywords": [
        "API",
        "Documentation",
        "Laravel"
    ],
    "homepage": "http://github.com/ovac/idoc",
    "authors": [
        {
            "name": "Victor Ariama (ovac4u)",
            "email": "iamovac@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "mpociot/laravel-apidoc-generator": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.5.* || 3.6.* || 3.7.*",
        "phpunit/phpunit": "^6.0.0 || ^7.4.0",
        "dingo/api": "2.0.0-alpha1",
        "mockery/mockery": "^1.2.0",
        "league/fractal": "^0.17.0"
    },
    "autoload": {
        "psr-4": {
            "OVAC\\IDoc\\": "src/idoc"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OVAC\\IDoc\\IDocServiceProvider"
            ]
       }
    }
}
